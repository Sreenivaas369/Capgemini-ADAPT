import java.util.Scanner;

public class Source {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		if(a>b && a>c && b>c) {
			if(a*a==b*b+c*c) {
				System.out.println("RIGHT ANGLE");
			}
			else {
				System.out.println("NOT RIGHT ANGLE");
			}
		}
		else {
			System.out.println("INVALID_INPUT");
		}
	}
}
